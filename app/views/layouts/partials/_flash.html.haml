%div.container
  - unless flash[:notice].nil?
    %div.alert.alert-info.alert-dismissable
      = flash[:notice]
      %button.close{ data: { dismiss: "alert" }} &times;
  - unless flash[:alert].nil?
    %div.alert.alert-warning.alert-dismissable
      = flash[:alert]
      %button.close{ data: { dismiss: "alert" }} &times;
